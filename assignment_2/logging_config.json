{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "standard": {
            "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
        }
    },
    "handlers": {
        "file_handler": {
            "class": "logging.handlers.RotatingFileHandler",
            "formatter": "standard",
            "filename": "./vllm_server.log",
            "maxBytes": 10485760,
            "backupCount": 5
        }
    },
    "loggers": {
        "vllm": {
            "handlers": ["file_handler"],
            "level": "INFO",
            "propagate": false
        }
    },
    "root": {
        "handlers": ["file_handler"],
        "level": "WARNING"
    }
}